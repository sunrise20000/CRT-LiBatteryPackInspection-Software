    <Canvas Name="Base" RenderTransformOrigin="0.5,0.5" HorizontalAlignment="Left" VerticalAlignment="Top">
        <Canvas.RenderTransform>
            <TransformGroup>
                <ScaleTransform/>
                <SkewTransform/>
                <RotateTransform Angle="-90"/>
                <TranslateTransform/>
            </TransformGroup>
        </Canvas.RenderTransform>
        <Image Source="/Crt.UiCore;component/Resources/Robot/yamaha_scara_base.png" />

        <Canvas Name="Arm1" Height="78" Width="236" Canvas.Left="-171.767" Canvas.Top="5.5" HorizontalAlignment="Center" VerticalAlignment="Center" RenderTransformOrigin="0.86,0.5">
            <Canvas.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="0"/>
                    <TranslateTransform/>
                </TransformGroup>
            </Canvas.RenderTransform>
            <Image Source="/Crt.UiCore;component/Resources/Robot/yamaha_scara_arm1.png"/>


            <Canvas Name="Arm2" Height="64" Width="238" HorizontalAlignment="Left" Canvas.Left="-167.6" VerticalAlignment="Top" Canvas.Top="7.6" RenderTransformOrigin="0.863,0.5">
                <Canvas.RenderTransform>
                    <TransformGroup>
                        <ScaleTransform/>
                        <SkewTransform/>
                        <RotateTransform/>
                        <TranslateTransform/>
                    </TransformGroup>
                </Canvas.RenderTransform>
                <Image Source="/Crt.UiCore;component/Resources/Robot/yamaha_scara_arm2.png"/>

                <Canvas Name="Gripper" Height="71" Width="182" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="-56,0,0,0" RenderTransformOrigin="0.5,0.5">
                    <Canvas.RenderTransform>
                        <TransformGroup>
                            <ScaleTransform/>
                            <SkewTransform/>
                            <RotateTransform Angle="50"/>
                            <TranslateTransform/>
                        </TransformGroup>
                    </Canvas.RenderTransform>
                    <Image Source="/Crt.UiCore;component/Resources/Robot/gripper.png" />

                    <local:LiBatteryPack IsShowBattery="{Binding HasBattery, RelativeSource={RelativeSource AncestorType=ContentControl, Mode=FindAncestor}, Mode=OneWay}" 
                                         Height="47" Width="147" 
                                         HorizontalAlignment="Center" 
                                         VerticalAlignment="Center" 
                                         Margin="15,12,0,0"/>

                </Canvas>

            </Canvas>

        </Canvas>
    </Canvas>
</local:BatteryCarrierBase>
