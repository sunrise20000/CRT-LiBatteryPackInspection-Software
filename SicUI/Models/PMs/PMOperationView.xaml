    <UserControl.Resources>
        <pm:InverseBoolConverter x:Key="InverseBoolConverter" />
        <converters:bool2VisibilityConverter x:Key="bool2VisibilityConvert" />
        <converters:bool2VisibilityConverter2 x:Key="bool2VisibilityConvert2" />

        <Storyboard x:Key="SlitValve_Open">
            <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(FrameworkElement.Height)">
                <EasingDoubleKeyFrame KeyTime="0" Value="75" />
                <EasingDoubleKeyFrame KeyTime="0:0:0.5" Value="55" />
            </DoubleAnimationUsingKeyFrames>
        </Storyboard>
        <Storyboard x:Key="SlitValve_Close">
            <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(FrameworkElement.Height)">
                <EasingDoubleKeyFrame KeyTime="0" Value="55" />
                <EasingDoubleKeyFrame KeyTime="0:0:0.5" Value="75" />
            </DoubleAnimationUsingKeyFrames>
        </Storyboard>
        <Style TargetType="{x:Type Label}">
            <Setter Property="FontSize" Value="10" />
        </Style>

        <Style x:Key="SlitValve_Animation" TargetType="{x:Type Rectangle}">
            <Style.Triggers>
                <DataTrigger Binding="{Binding OpenFeedback}" Value="true">
                    <DataTrigger.EnterActions>
                        <BeginStoryboard Storyboard="{StaticResource SlitValve_Open}" />
                    </DataTrigger.EnterActions>
                    <DataTrigger.ExitActions>
                        <BeginStoryboard Storyboard="{StaticResource SlitValve_Close}" />
                    </DataTrigger.ExitActions>
                </DataTrigger>
            </Style.Triggers>
        </Style>
        <system:Double x:Key="Pipe_Height">2</system:Double>
        <Color x:Key="CenterColorYellow">GhostWhite</Color>
        <Color x:Key="CenterColorWhite">LightGray</Color>
        <Color x:Key="CenterColorPurple">White</Color>
        <Color x:Key="CenterColorRed">White</Color>
        <Color x:Key="EdgeColorYellow">Gold</Color>
        <Color x:Key="EdgeColorWhite">DimGray</Color>
        <Color x:Key="EdgeColorRed">Red</Color>
        <Color x:Key="EdgeColorPurple">Purple</Color>
        <Color x:Key="EdgeColorGreen">LightGreen</Color>
        <Color x:Key="EdgeColorBlue">#145b7d</Color>
        <Color x:Key="EdgeColorGray">Gray</Color>
        <Color x:Key="EdgeColorBlack">Black</Color>
        <Color x:Key="EdgeColorLightGreen">#00ae9d</Color>


        <Style TargetType="controlRT:AnalogControl">
            <Setter Property="HasPermission" Value="{Binding HasMfcValueVisibilityPermission, Mode=OneWay}"/>
        </Style>

        <Style TargetType="controlRT:AnalogControl2">
            <Setter Property="HasPermission" Value="{Binding HasMfcValueVisibilityPermission, Mode=OneWay}"/>
            <Setter Property="Disturbance" Value="{Binding MfcValueDisturbance, Mode=OneWay}"/>
        </Style>

        <Style TargetType="controlRT:ValveBig">
            <Setter Property="HasPermission" Value="{Binding HasMfcValueVisibilityPermission, Mode=OneWay}"/>
        </Style>

        <Style TargetType="deviceControl:AITThrottleValve2">
