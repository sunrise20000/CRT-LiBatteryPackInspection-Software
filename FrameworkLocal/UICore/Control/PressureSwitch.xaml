<UserControl
    x:Class="Aitex.Core.UI.Control.PressureSwitch"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Width="10"
    Height="10"
    MinWidth="10"
    MinHeight="10"
    MaxWidth="10"
    MaxHeight="10"
    Opacity="1">
    <UserControl.RenderTransform>
        <RotateTransform x:Name="rotateTransform" CenterX="5" CenterY="5" />
    </UserControl.RenderTransform>
    <Canvas>
        <Ellipse
            x:Name="psEllipse"
            Width="10"
            Height="10"
            Panel.ZIndex="1000"
            Fill="Green"
            Opacity="1"
            Stroke="Black" />
    </Canvas>
</UserControl>
